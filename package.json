{
  "name": "humanos-acquisition",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "nodemon server",
    "build": "next build",
    "serve": "NODE_ENV=production node server",
    "lint": "cross-env CI=true eslint './{pages,server,utils}/**/*.{ts,tsx,json}'",
    "lint:fix": "yarn lint --fix",
    "typecheck": "tsc -p .",
    "optimize:svg": "yarn optimize svg"
  },
  "dependencies": {
    "@emotion/core": "10.0.27",
    "@emotion/styled": "10.0.27",
    "@sentry/browser": "5.12.4",
    "@sentry/node": "5.12.4",
    "@startupcraft/dotenv-config": "0.0.2",
    "addressparser": "1.0.1",
    "axios": "0.19.2",
    "emailjs-imap-client": "3.1.0",
    "js-cookie": "2.2.1",
    "koa": "2.11.0",
    "koa-compress": "3.0.0",
    "koa-router": "8.0.8",
    "lodash": "4.17.15",
    "nanoid": "2.1.11",
    "next": "9.2.2",
    "next-images": "1.3.1",
    "qs": "6.9.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-google-login": "5.1.1",
    "rebass": "4.0.7",
    "socket.io": "2.3.0",
    "socket.io-client": "2.3.0",
    "typescript": "3.8.2"
  },
  "devDependencies": {
    "@types/jest": "25.1.3",
    "@types/js-cookie": "2.2.4",
    "@types/lodash": "4.14.149",
    "@types/nanoid": "2.1.0",
    "@types/node": "13.7.4",
    "@types/qs": "6.9.1",
    "@types/react": "16.9.22",
    "@types/react-dom": "16.9.5",
    "@types/rebass": "4.0.4",
    "@types/socket.io": "2.1.4",
    "@types/socket.io-client": "1.4.32",
    "@typescript-eslint/eslint-plugin": "2.21.0",
    "@typescript-eslint/parser": "2.21.0",
    "babel-plugin-emotion": "10.0.27",
    "cross-env": "7.0.0",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-json": "2.1.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "nodemon": "2.0.2",
    "prettier": "1.19.1"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,json}": [
      "eslint --fix",
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn typecheck && lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
